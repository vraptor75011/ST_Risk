{
  "name": "graphqlserver",
  "version": "0.0.1",
  "description": "graphqlServer for ST_Risk backend",
  "main": "index.js",
  "scripts": {
    "dev": "concurrently \" nodemon ./dist/bundle.js \" \" webpack --watch --config webpack.config.js\" ",
    "build": "webpack",
    "start": "",
    "migrate:up": "knex --knexfile ./src/database/knexfile.ts migrate:latest",
    "generate:types": "graphql-codegen --config codegen.yml",
    "test": "DEBUG=testcontainers jest --detectOpenHandles --coverage",
    "typeorm": "node --require ts-node/register ./node_modules/typeorm/cli.js",
    "typeorm:cli": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli -f ./src/database/utils/orm.config.migrations.ts",
    "typeorm:cli2": "node --require ts-node/register ./node_modules/typeorm/cli -f ./src/database/utils/orm.config.migrations.ts",
     "migration-generate": "yarn run typeorm:cli2 migration:generate -n",
     "migration-run": "yarn run typeorm:cli2 migration:run"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@types/pg": "^7.14.6",
    "apollo-server-koa": "^2.18.2",
    "dotenv": "^8.2.0",
    "graphql": "^15.3.0",
    "graphql-tools": "^6.2.4",
    "koa": "^2.13.0",
    "koa-router": "^9.4.0",
    "pg": "^8.5.1",
    "reflect-metadata": "^0.1.13",
    "type-graphql": "^1.1.0",
    "typeorm": "^0.2.28"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "^1.17.10",
    "@graphql-codegen/typescript": "^1.17.10",
    "@graphql-codegen/typescript-operations": "^1.17.8",
    "@graphql-codegen/typescript-resolvers": "^1.17.10",
    "@types/find-config": "^1.0.0",
    "@types/graphql": "^14.5.0",
    "@types/jest": "^26.0.15",
    "@types/koa": "^2.11.4",
    "@types/koa-router": "^7.4.1",
    "@types/node": "^14.11.8",
    "awesome-typescript-loader": "^5.2.1",
    "concurrently": "^5.3.0",
    "graphql-tag": "^2.11.0",
    "jest": "^26.6.3",
    "nodemon": "^2.0.4",
    "testcontainers": "^6.1.0",
    "ts-jest": "^26.4.4",
    "ts-node": "^9.0.0",
    "typescript": "^4.0.3",
    "webpack": "^5.0.0",
    "webpack-cli": "^4.0.0",
    "webpack-node-externals": "^2.5.2"
  }
}
