{
  "name": "graphqlserver",
  "version": "0.0.1",
  "description": "graphqlServer for ST_Risk backend",
  "main": "index.js",
  "scripts": {
    "dev:create-index": "ts-node-dev --respawn ./tools/createTypeOrmIndexes.ts",
    "dev:dev": "ts-node-dev --project './tsconfig.json' --respawn ./src/index.ts",
    "dev": "concurrently -n \"Indexes, Dev \" \" yarn dev:create-index \" \" yarn dev:dev \" ",
    "dev:webpack": "concurrently \" nodemon ./dist/bundle.js \" \" webpack --watch --config webpack.config.js\" ",
    "build": "webpack",
    "test": "DEBUG=testcontainers jest --detectOpenHandles --watchAll --runInBand --coverage",
    "typeorm": "node --require ts-node/register ./node_modules/typeorm/cli.js",
    "typeorm:cli": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli -f ./src/database/utils/orm.config.migrations.ts",
    "typeorm:cli2": "node --require ts-node/register ./node_modules/typeorm/cli -f ./src/database/utils/orm.config.migrations.ts",
    "migration-generate": "yarn run typeorm:cli2 migration:generate -n",
    "migration-run": "yarn run typeorm:cli2 migration:run",
    "build:dev": "concurrently \" nodemon ./dist/backend.js \" \" webpack --config webpack.back.development.js\" ",
    "build:prod": "webpack --config webpack.back.production.js",
    "start": "node dist/server"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "apollo-server-koa": "^2.18.2",
    "class-validator": "^0.12.2",
    "dotenv": "^8.2.0",
    "graphql": "^15.3.0",
    "graphql-tools": "^6.2.4",
    "koa": "^2.13.0",
    "koa-router": "^9.4.0",
    "pg": "^8.5.1",
    "reflect-metadata": "^0.1.13",
    "type-graphql": "^1.1.0",
    "typeorm": "^0.2.29"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "^1.17.10",
    "@graphql-codegen/typescript": "^1.17.10",
    "@graphql-codegen/typescript-operations": "^1.17.8",
    "@graphql-codegen/typescript-resolvers": "^1.17.10",
    "@types/faker": "^5.1.4",
    "@types/find-config": "^1.0.0",
    "@types/graphql": "^14.5.0",
    "@types/jest": "^26.0.15",
    "@types/koa": "^2.11.4",
    "@types/koa-router": "^7.4.1",
    "@types/node": "^14.11.8",
    "@types/pg": "^7.14.6",
    "circular-dependency-plugin": "^5.2.0",
    "clean-webpack-plugin": "^3.0.0",
    "concurrently": "^5.3.0",
    "copy-webpack-plugin": "^6.3.2",
    "factory.ts": "^0.5.1",
    "faker": "^5.1.0",
    "graphql-tag": "^2.11.0",
    "jest": "^26.6.3",
    "jest-date": "^1.1.4",
    "nodemon": "^2.0.4",
    "testcontainers": "^6.1.0",
    "ts-jest": "^26.4.4",
    "ts-loader": "^8.0.11",
    "ts-node": "^9.0.0",
    "ts-node-dev": "^1.0.0",
    "typedi": "^0.8.0",
    "typeorm-typedi-extensions": "^0.2.3",
    "typescript": "^4.1.2",
    "watch-glob": "^0.1.3",
    "webpack": "^5.0.0",
    "webpack-cli": "^4.0.0",
    "webpack-graphql-loader": "^1.0.2",
    "webpack-merge": "^5.4.0",
    "webpack-node-externals": "^2.5.2"
  }
}
